{
  "name": "nzxt-code",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "validate-commit-msg": "node ./node_modules/@commitlint/cli/lib/cli.js --edit $1",
    "clean": "node ./node_modules/rimraf/bin.js  build ",
    "clean-swagger-doc": "node src/build_util/swagger_clean.js ",
    "clean-swagger": "node  ./node_modules/rimraf/bin.js  ./build/swagger.json ",
    "clean-route": "node ./node_modules/rimraf/bin.js ./src/http/routes/routes.ts ",
    "build": "npm run clean && npm run build-route && npm run build-swagger && tsc ",
    "build-route": "npm run clean-route && tsoa routes ",
    "build-swagger": "npm run clean-swagger && tsoa swagger ",
    "check-docker-http": "node ./node_modules/rimraf/bin.js node_modules/* && npm install --production && npm ruin build && npm run http",
    "-------------------- development --------------------": "",
    "dev-http": " nodemon --watch ./src --delay 3 --ext ts --exec ts-node --ignore '*.test.ts' src/http/index.ts",
    "-------------------- test --------------------": "",
    "test": "node ./node_modules/jest/bin/jest.js --coverage",
    "test-debug": "node ./node_modules/jest/bin/jest.js --coverage  --detectOpenHandles",
    "-------------------- production --------------------": "",
    "start": "node --no-deprecation ./build/http",
    "http": "node --no-deprecation ./build/http",
    "-------------------- release --------------------": "",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "-------------------- git --------------------": "",
    "git-log": "git log --oneline",
    "-------------------- formatter --------------------": "",
    "lint": "eslint . --ext .js,.ts --ignore-path .eslintignore",
    "lint-fix": "eslint . --fix",
    "prettier-fix": "npx prettier --write src/**/*.ts ",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pollychang0609/NZXT-CODE.git"
  },
  "keywords": [],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts"
    ],
    "coverageDirectory": "coverage"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/pollychang0609/NZXT-CODE#readme",
  "dependencies": {
    "@jest/types": "^26.6.2",
    "@types/aes-js": "^3.1.1",
    "@types/connect-redis": "^0.0.16",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/supertest": "^2.0.11",
    "aes-js": "^3.1.2",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "eslint-plugin-jest": "^24.3.6",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "fs": "^0.0.1-security",
    "jest": "^26.6.3",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.13",
    "nodemon": "^2.0.7",
    "npm": "^7.18.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.0.0",
    "tsoa": "^3.8.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "@shelf/jest-mongodb": "^1.2.5",
    "@types/express": "^4.17.12",
    "@types/mongoose": "^5.11.97",
    "@types/passport": "^1.0.6",
    "@types/ws": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-plugin-eslint-plugin": "^3.0.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "os": "^0.1.1",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "typescript-eslint-parser": "^22.0.0"
  }
}
